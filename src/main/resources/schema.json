{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "data",
  "description": "population data for usa",
  "type": "object",
  "properties": {
    "Year": {
      "description": "The year that the population data was taken in.",
      "type": "string"
    },
    "ID Nation": {
      "description": "Identification code for the nation.",
      "type": "string"
    },
    "Nation": {
      "description": "Name of the nation.",
      "type": "string"
    },
    "Slug Nation": {
      "description": "Readable unique identifier for the nation.",
      "type": "string"
    },
    "ID Year": {
      "description": "The year that the population data was taken in.",
      "type": "integer"
    },
    "Population": {
      "type": "number",
      "minimum": 0,
      "exclusiveMinimum": true
    }
  },
  "required": ["Year", "ID Year", "Population"]
}